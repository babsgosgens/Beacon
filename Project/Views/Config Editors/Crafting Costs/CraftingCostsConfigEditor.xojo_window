		  If Self.List.SelCount = 1 Then
		    Self.List.Cell(Self.List.ListIndex, 0) = Me.Target.Label
		    Self.List.Sort
		    Self.ContentsChanged = True
		  End If
		End Sub
	#tag EndEvent
